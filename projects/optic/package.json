{
  "name": "@useoptic/optic",
  "packageManager": "yarn@3.0.2",
  "version": "0.34.2",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "/build",
    "/src"
  ],
  "bin": {
    "oas": "build/oas.js",
    "optic": "build/index.js"
  },
  "scripts": {
    "dev:test": "ENVIRONMENT=test jest --colors",
    "build": "yarn tsc --build --verbose && yarn babel ./src --out-dir ./build --extensions \".ts\",\".tsx\"",
    "ws:build": "yarn build",
    "ws:clean": "rm -rf build",
    "ws:test": "yarn dev:test",
    "local:run": "yarn ts-node src/index.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.17.0",
    "@babel/preset-react": "^7.17.0",
    "@babel/preset-typescript": "^7.17.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/brotli": "^1.3.1",
    "@types/is-url": "^1.2.30",
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.5",
    "@types/json-stable-stringify": "^1.0.33",
    "@types/node": "^17.0.15",
    "@types/picomatch": "^2.3.0",
    "@types/url-join": "^4.0.1",
    "@types/uuid": "^8.3.1",
    "babel-jest": "^27.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "jest": "^27.5.1",
    "prettier": "^2.4.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@octokit/rest": "^18.12.0",
    "@sentry/node": "^7.10.0",
    "@stoplight/spectral-core": "^1.8.1",
    "@useoptic/openapi-cli": "workspace:*",
    "@useoptic/openapi-io": "workspace:*",
    "@useoptic/openapi-utilities": "workspace:*",
    "@useoptic/optic-ci": "workspace:*",
    "@useoptic/rulesets-base": "workspace:*",
    "@useoptic/standard-rulesets": "workspace:*",
    "ajv": "^8.11.0",
    "bottleneck": "^2.19.5",
    "brotli": "^1.3.3",
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "dotenv": "^16.0.2",
    "is-url": "^1.2.4",
    "js-yaml": "^4.1.0",
    "json-stable-stringify": "^1.0.1",
    "loglevel": "^1.8.0",
    "node-fetch": "^2.6.7",
    "node-machine-id": "^1.1.12",
    "open": "^8.4.0",
    "picomatch": "^2.3.1",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "build"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "^nimma/fallbacks$": [
        "<rootDir>../../node_modules/nimma/dist/cjs/fallbacks/index.js",
        "<rootDir>../../../../node_modules/nimma/dist/cjs/fallbacks/index.js"
      ],
      "^nimma/legacy$": [
        "<rootDir>../../node_modules/nimma/dist/legacy/cjs/index.js",
        "<rootDir>../../../../node_modules/nimma/dist/legacy/cjs/index.js"
      ]
    }
  }
}
