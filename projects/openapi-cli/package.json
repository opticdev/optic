{
  "name": "@useoptic/openapi-cli",
  "packageManager": "yarn@3.0.2",
  "version": "0.24.22",
  "main": "build/lib.js",
  "types": "build/lib.d.ts",
  "files": [
    "/build"
  ],
  "bin": "build/cli.js",
  "scripts": {
    "dev:test": "jest --colors",
    "build": "yarn tsc --build --verbose && yarn babel ./src --out-dir ./build --extensions \".ts\",\".tsx\"",
    "ws:build": "yarn build",
    "ws:clean": "rm -rf build",
    "ws:test": "yarn dev:test"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/analytics-node": "^3.1.7",
    "@types/babel__core": "^7",
    "@types/is-url": "^1.2.30",
    "@types/jest": "^26.0.24",
    "@types/json-stable-stringify": "^1.0.33",
    "@types/node": "^17.0.15",
    "@types/stream-chain": "^2",
    "@types/stream-json": "^1",
    "babel-jest": "^27.5.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "jest": "^27.5.1",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@useoptic/json-pointer-helpers": "workspace:*",
    "@useoptic/openapi-io": "workspace:*",
    "@useoptic/openapi-utilities": "workspace:*",
    "analytics-node": "^6.0.0",
    "axax": "^0.2.2",
    "commander": "^9.0.0",
    "fast-deep-equal": "^3.1.3",
    "is-url": "^1.2.4",
    "json-schema-traverse": "^1.0.0",
    "node-machine-id": "^1.1.12",
    "stream-chain": "^2.2.5",
    "stream-json": "^1.7.4",
    "ts-invariant": "^0.9.4",
    "ts-results": "^3.3.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "build"
    ],
    "moduleNameMapper": {
      "^nimma/fallbacks$": [
        "<rootDir>../../node_modules/nimma/dist/cjs/fallbacks/index.js",
        "<rootDir>../../../../node_modules/nimma/dist/cjs/fallbacks/index.js"
      ],
      "^nimma/legacy$": [
        "<rootDir>../../node_modules/nimma/dist/legacy/cjs/index.js",
        "<rootDir>../../../../node_modules/nimma/dist/legacy/cjs/index.js"
      ],
      "csv-parse/sync": [
        "<rootDir>../../node_modules/csv-parse/dist/cjs/sync.cjs",
        "<rootDir>../../../../node_modules/csv-parse/dist/cjs/sync.cjs"
      ]
    }
  }
}
