{
  "name": "openapi-workspaces",
  "private": true,
  "version": "0.34.2",
  "workspaces": [
    "projects/json-pointer-helpers",
    "projects/openapi-io",
    "projects/openapi-utilities",
    "projects/rulesets-base",
    "projects/optic",
    "projects/optic-ci",
    "projects/openapi-cli",
    "projects/standard-rulesets"
