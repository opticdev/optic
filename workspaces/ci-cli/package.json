{
  "name": "@useoptic/ci-cli",
  "version": "8.2.9",
  "author": "@useoptic",
  "bin": {
    "optic-ci": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/useoptic/optic-package.git",
    "directory": "workspaces/ci-cli"
  },
  "bugs": "https://github.com/opticdev/optic/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@useoptic/cli-config": "8.2.9",
    "@useoptic/cli-shared": "8.2.9",
    "dotenv": "^8.2.0",
    "jwt-decode": "^2.2.0",
    "tslib": "^1",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^12",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "fs-extra": "^9.0.0",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/opticdev/optic",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "optic-ci",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "ws:build": "tsc -b --verbose",
    "ws:clean": "rm -rf lib",
    "ws:test": "echo skipping tests"
  },
  "types": "lib/index.d.ts"
}
